      </button>
    </div>
  </div>
</category-slider-{{ ai_gen_id }}>

<script>
  (function() {
    class CategorySlider{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.currentIndex = 0;
        this.itemsPerView = {{ block.settings.desktop_columns }};
      }

      connectedCallback() {
        if (window.innerWidth >= 750) {
          this.track = this.querySelector('.ai-category-section__slider-track-{{ ai_gen_id }}');
          this.prevButton = this.querySelector('.ai-category-section__arrow--prev-{{ ai_gen_id }}');
          this.nextButton = this.querySelector('.ai-category-section__arrow--next-{{ ai_gen_id }}');
          this.items = this.querySelectorAll('.ai-category-section__slider-item-{{ ai_gen_id }}');
          this.totalItems = this.items.length;

          if (this.totalItems > 0) {
            this.setupEventListeners();
            this.updateButtons();
          }
        }
      }

      setupEventListeners() {
        this.prevButton.addEventListener('click', () => this.slide(-1));
        this.nextButton.addEventListener('click', () => this.slide(1));

        document.addEventListener('keydown', (e) => {
          if (e.key === 'ArrowLeft') {
            this.slide(-1);
          } else if (e.key === 'ArrowRight') {
            this.slide(1);
          }
        });
      }

      slide(direction) {
        const maxIndex = Math.max(0, this.totalItems - this.itemsPerView);
        this.currentIndex = Math.max(0, Math.min(maxIndex, this.currentIndex + direction));
        this.updateSlider();
        this.updateButtons();
      }

      updateSlider() {
        const itemWidth = this.items[0].offsetWidth;
        const gap = {{ block.settings.grid_gap }};
        const offset = -(this.currentIndex * (itemWidth + gap));
        this.track.style.transform = `translateX(${offset}px)`;
      }

      updateButtons() {
        const maxIndex = Math.max(0, this.totalItems - this.itemsPerView);
        this.prevButton.disabled = this.currentIndex === 0;
        this.nextButton.disabled = this.currentIndex >= maxIndex;
      }
    }

    customElements.define('category-slider-{{ ai_gen_id }}', CategorySlider{{ ai_gen_id }});
  })();
</script>

