                            width="600"
                            height="{{ 600 | times: block.settings.image_aspect_ratio | round }}"
                          >
                        {% else %}
                          <div class="ai-category-section__placeholder-{{ ai_gen_id }}">
                            {{ 'collection-1' | placeholder_svg_tag }}
                          </div>
                        {% endif %}
