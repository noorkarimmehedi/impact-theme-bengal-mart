{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-category-section-{{ ai_gen_id }} {
    padding-top: {{ block.settings.section_padding_top }}px;
    padding-bottom: {{ block.settings.section_padding_bottom }}px;
    background-color: {{ block.settings.section_bg_color }};
  }

  .ai-category-section__title-{{ ai_gen_id }} {
    text-align: center;
    margin: 0 0 {{ block.settings.title_margin_bottom }}px;
    font-size: {{ block.settings.title_size }}px;
    color: {{ block.settings.title_color }};
  }

  .ai-category-section__container-{{ ai_gen_id }} {
    max-width: 1400px;
    margin: 0 auto;
    padding: 0 20px;
  }

  .ai-category-section__grid-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: {{ block.settings.grid_gap }}px;
  }

  .ai-category-section__item-{{ ai_gen_id }} {
    position: relative;
    overflow: hidden;
    border-radius: {{ block.settings.border_radius }}px;
    border: {{ block.settings.border_width }}px solid {{ block.settings.border_color }};
    transition: all 0.3s ease;
    box-shadow: {{ block.settings.shadow_horizontal }}px {{ block.settings.shadow_vertical }}px {{ block.settings.shadow_blur }}px rgba(0, 0, 0, {{ block.settings.shadow_opacity }});
  }

  .ai-category-section__item-{{ ai_gen_id }}:hover {
    border-color: {{ block.settings.hover_border_color }};
  }

  .ai-category-section__link-{{ ai_gen_id }} {
    display: block;
    position: relative;
    text-decoration: none;
    color: inherit;
  }

  .ai-category-section__image-wrapper-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    padding-bottom: {{ block.settings.image_aspect_ratio | times: 100 }}%;
    overflow: hidden;
  }

  .ai-category-section__image-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.3s ease;
  }

  .ai-category-section__item-{{ ai_gen_id }}:hover .ai-category-section__image-{{ ai_gen_id }} {
    transform: scale(1.05);
  }

  .ai-category-section__placeholder-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: #f4f4f4;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-category-section__placeholder-{{ ai_gen_id }} svg {
    width: 60%;
    height: 60%;
    opacity: 0.3;
  }

  .ai-category-section__title-wrapper-{{ ai_gen_id }} {
    {% if block.settings.overlay_text %}
      position: absolute;
      bottom: 0;
      left: 0;
      right: 0;
      padding: 20px;
      z-index: 2;
    {% else %}
      padding: 15px;
      background-color: {{ block.settings.title_bg_color }};
    {% endif %}
  }

  .ai-category-section__category-title-{{ ai_gen_id }} {
    margin: 0;
    font-size: {{ block.settings.category_title_size }}px;
    color: {{ block.settings.category_title_color }};
    text-align: {{ block.settings.text_align }};
    {% if block.settings.overlay_text %}
      text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3);
    {% endif %}
  }

  .ai-category-section__desktop-slider-{{ ai_gen_id }} {
    display: none;
  }

  @media screen and (min-width: 750px) {
    .ai-category-section__grid-{{ ai_gen_id }} {
      display: none;
    }

    .ai-category-section__desktop-slider-{{ ai_gen_id }} {
      display: block;
      position: relative;
    }

    .ai-category-section__slider-wrapper-{{ ai_gen_id }} {
      overflow: hidden;
      position: relative;
    }

    .ai-category-section__slider-track-{{ ai_gen_id }} {
      display: flex;
      gap: {{ block.settings.grid_gap }}px;
      transition: transform 0.4s ease;
    }

    .ai-category-section__slider-item-{{ ai_gen_id }} {
      flex: 0 0 calc((100% - ({{ block.settings.desktop_columns | minus: 1 }} * {{ block.settings.grid_gap }}px)) / {{ block.settings.desktop_columns }});
      min-width: 0;
    }

    .ai-category-section__arrow-{{ ai_gen_id }} {
      position: absolute;
      top: 50%;
      transform: translateY(-50%);
      background-color: {{ block.settings.arrow_bg_color }};
      color: {{ block.settings.arrow_color }};
      border: none;
      width: 50px;
      height: 50px;
      border-radius: 50%;
      cursor: pointer;
      z-index: 10;
      display: flex;
      align-items: center;
      justify-content: center;
      transition: all 0.3s ease;
      box-shadow: 0 2px 8px rgba(0, 0, 0, 0.15);
    }

    .ai-category-section__arrow-{{ ai_gen_id }}:hover {
      background-color: {{ block.settings.arrow_hover_bg_color }};
      color: {{ block.settings.arrow_hover_color }};
    }

    .ai-category-section__arrow-{{ ai_gen_id }}:disabled {
      opacity: 0.3;
      cursor: not-allowed;
    }

    .ai-category-section__arrow--prev-{{ ai_gen_id }} {
      left: -25px;
    }

    .ai-category-section__arrow--next-{{ ai_gen_id }} {
      right: -25px;
    }

    .ai-category-section__arrow-{{ ai_gen_id }} svg {
      width: 24px;
      height: 24px;
    }
  }
{% endstyle %}

<category-slider-{{ ai_gen_id }} class="ai-category-section-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-category-section__container-{{ ai_gen_id }}">
    {% if block.settings.section_title != blank %}
      <h2 class="ai-category-section__title-{{ ai_gen_id }}">{{ block.settings.section_title }}</h2>
    {% endif %}

    <div class="ai-category-section__grid-{{ ai_gen_id }}">
      {% for i in (1..12) %}
        {% liquid
          assign collection_key = 'collection_' | append: i
          assign custom_image_key = 'custom_image_' | append: i
          assign custom_title_key = 'custom_title_' | append: i
          assign hide_key = 'hide_collection_' | append: i

          assign collection = block.settings[collection_key]
          assign custom_image = block.settings[custom_image_key]
          assign custom_title = block.settings[custom_title_key]
          assign hide_collection = block.settings[hide_key]
        %}

        {% if hide_collection == false %}
          {% if collection != blank or custom_image != blank or custom_title != blank %}
            <div class="ai-category-section__item-{{ ai_gen_id }}">
              {% if collection != blank %}
                <a href="{{ collection.url }}" class="ai-category-section__link-{{ ai_gen_id }}">
              {% else %}
                <div class="ai-category-section__link-{{ ai_gen_id }}">
              {% endif %}
                <div class="ai-category-section__image-wrapper-{{ ai_gen_id }}">
                  {% if custom_image != blank %}
                    <img
                      src="{{ custom_image | image_url: width: 600 }}"
                      alt="{{ custom_title | default: collection.title | escape }}"
                      class="ai-category-section__image-{{ ai_gen_id }}"
                      loading="lazy"
                      width="600"
                      height="{{ 600 | times: block.settings.image_aspect_ratio | round }}"
                    >
                  {% elsif collection != blank and collection.featured_image != blank %}
                    <img
                      src="{{ collection.featured_image | image_url: width: 600 }}"
                      alt="{{ collection.title | escape }}"
                      class="ai-category-section__image-{{ ai_gen_id }}"
                      loading="lazy"
                      width="600"
                      height="{{ 600 | times: block.settings.image_aspect_ratio | round }}"
                    >
                  {% else %}
                    <div class="ai-category-section__placeholder-{{ ai_gen_id }}">
                      {{ 'collection-1' | placeholder_svg_tag }}
                    </div>
                  {% endif %}
                </div>
                <div class="ai-category-section__title-wrapper-{{ ai_gen_id }}">
                  <h3 class="ai-category-section__category-title-{{ ai_gen_id }}">
                    {{ custom_title | default: collection.title | default: 'Category Name' }}
                  </h3>
                </div>
              {% if collection != blank %}
                </a>
              {% else %}
                </div>
              {% endif %}
            </div>
          {% endif %}
        {% endif %}
      {% endfor %}
    </div>

    <div class="ai-category-section__desktop-slider-{{ ai_gen_id }}">
      <button class="ai-category-section__arrow-{{ ai_gen_id }} ai-category-section__arrow--prev-{{ ai_gen_id }}" aria-label="Previous">
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
          <polyline points="15 18 9 12 15 6"></polyline>
        </svg>
      </button>
      <div class="ai-category-section__slider-wrapper-{{ ai_gen_id }}">
        <div class="ai-category-section__slider-track-{{ ai_gen_id }}">
          {% for i in (1..12) %}
            {% liquid
              assign collection_key = 'collection_' | append: i
              assign custom_image_key = 'custom_image_' | append: i
              assign custom_title_key = 'custom_title_' | append: i
              assign hide_key = 'hide_collection_' | append: i

              assign collection = block.settings[collection_key]
              assign custom_image = block.settings[custom_image_key]
              assign custom_title = block.settings[custom_title_key]
              assign hide_collection = block.settings[hide_key]
            %}

            {% if hide_collection == false %}
              {% if collection != blank or custom_image != blank or custom_title != blank %}
                <div class="ai-category-section__slider-item-{{ ai_gen_id }}">
                  <div class="ai-category-section__item-{{ ai_gen_id }}">
                    {% if collection != blank %}
                      <a href="{{ collection.url }}" class="ai-category-section__link-{{ ai_gen_id }}">
                    {% else %}
                      <div class="ai-category-section__link-{{ ai_gen_id }}">
                    {% endif %}
                      <div class="ai-category-section__image-wrapper-{{ ai_gen_id }}">
                        {% if custom_image != blank %}
                          <img
                            src="{{ custom_image | image_url: width: 600 }}"
                            alt="{{ custom_title | default: collection.title | escape }}"
                            class="ai-category-section__image-{{ ai_gen_id }}"
                            loading="lazy"
                            width="600"
                            height="{{ 600 | times: block.settings.image_aspect_ratio | round }}"
                          >
                        {% elsif collection != blank and collection.featured_image != blank %}
                          <img
                            src="{{ collection.featured_image | image_url: width: 600 }}"
                            alt="{{ collection.title | escape }}"
                            class="ai-category-section__image-{{ ai_gen_id }}"
