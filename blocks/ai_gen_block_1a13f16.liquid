{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  {{ block.settings.title_font | font_face: font_display: 'swap' }}

  .ai-category-section-{{ ai_gen_id }} {
    padding: {{ block.settings.padding_top }}px 20px {{ block.settings.padding_bottom }}px;
  }

  .ai-category-title-{{ ai_gen_id }} {
    text-align: center;
    margin-bottom: 30px;
    font-size: 32px;
    font-weight: 600;
    font-family: {{ block.settings.title_font.family }}, {{ block.settings.title_font.fallback_families }};
    font-weight: {{ block.settings.title_font.weight }};
    font-style: {{ block.settings.title_font.style }};
    position: relative;
    display: inline-block;
    width: 100%;
    cursor: default;
    user-select: none;
  }

  .ai-category-title-wrapper-{{ ai_gen_id }} {
    text-align: center;
    margin-bottom: 30px;
    position: relative;
    display: inline-block;
    width: 100%;
  }

  .ai-category-title-inner-{{ ai_gen_id }} {
    position: relative;
    display: inline-block;
    transition: filter 250ms;
  }

  .ai-category-title-shadow-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    border-radius: 12px;
    background: hsl(0deg 0% 0% / 0.25);
    will-change: transform;
    transform: translateY(2px);
    transition: transform 600ms cubic-bezier(.3, .7, .4, 1);
  }

  .ai-category-title-edge-{{ ai_gen_id }} {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    border-radius: 12px;
    background: linear-gradient(
      to left,
      hsl(340deg 100% 16%) 0%,
      hsl(340deg 100% 32%) 8%,
      hsl(340deg 100% 32%) 92%,
      hsl(340deg 100% 16%) 100%
    );
  }

  .ai-category-title-front-{{ ai_gen_id }} {
    display: block;
    position: relative;
    padding: 12px 27px;
    border-radius: 12px;
    font-size: 32px;
    color: white;
    background: hsl(345deg 100% 47%);
    will-change: transform;
    transform: translateY(-4px);
    transition: transform 600ms cubic-bezier(.3, .7, .4, 1);
    font-family: {{ block.settings.title_font.family }}, {{ block.settings.title_font.fallback_families }};
    font-weight: {{ block.settings.title_font.weight }};
    font-style: {{ block.settings.title_font.style }};
  }

  .ai-category-title-inner-{{ ai_gen_id }}:hover {
    filter: brightness(110%);
  }

  .ai-category-title-inner-{{ ai_gen_id }}:hover .ai-category-title-front-{{ ai_gen_id }} {
    transform: translateY(-6px);
    transition: transform 250ms cubic-bezier(.3, .7, .4, 1.5);
  }

  .ai-category-title-inner-{{ ai_gen_id }}:hover .ai-category-title-shadow-{{ ai_gen_id }} {
    transform: translateY(4px);
    transition: transform 250ms cubic-bezier(.3, .7, .4, 1.5);
  }

  .ai-category-grid-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: {{ block.settings.gap_mobile }}px;
    max-width: 100%;
  }

  .ai-category-item-{{ ai_gen_id }} {
    position: relative;
    overflow: hidden;
    border: {{ block.settings.border_width }}px solid {{ block.settings.border_color }};
    border-radius: {{ block.settings.border_radius }}px;
    box-shadow: {{ block.settings.shadow_horizontal }}px {{ block.settings.shadow_vertical }}px {{ block.settings.shadow_blur }}px rgba(0, 0, 0, {{ block.settings.shadow_opacity | divided_by: 100.0 }});
    transition: all 0.3s ease;
  }

  {% if block.settings.image_aspect_ratio == 'square' %}
    .ai-category-item-{{ ai_gen_id }} {
      aspect-ratio: 1;
    }
  {% elsif block.settings.image_aspect_ratio == 'portrait' %}
    .ai-category-item-{{ ai_gen_id }} {
      aspect-ratio: 3 / 4;
    }
  {% elsif block.settings.image_aspect_ratio == 'landscape' %}
    .ai-category-item-{{ ai_gen_id }} {
      aspect-ratio: 4 / 3;
    }
  {% endif %}

  .ai-category-item-{{ ai_gen_id }}:hover {
    border-color: {{ block.settings.hover_border_color }};
  }

  .ai-category-link-{{ ai_gen_id }} {
    display: block;
    width: 100%;
    height: 100%;
    position: relative;
    text-decoration: none;
  }

  .ai-category-image-wrapper-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    position: relative;
  }

  .ai-category-image-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .ai-category-placeholder-{{ ai_gen_id }} {
    width: 100%;
    height: 100%;
    background-color: #f4f4f4;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .ai-category-placeholder-{{ ai_gen_id }} svg {
    width: 60%;
    height: 60%;
    opacity: 0.3;
  }

  .ai-category-overlay-{{ ai_gen_id }} {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    padding: 12px;
    background: linear-gradient(to top, rgba(0, 0, 0, {{ block.settings.overlay_background_opacity | divided_by: 100.0 }}), transparent);
    color: {{ block.settings.overlay_text_color }};
    text-align: center;
    font-size: 14px;
    font-weight: 600;
    z-index: 2;
    text-shadow: 0 1px 3px rgba(0, 0, 0, 0.5);
  }

  .ai-category-desktop-slider-{{ ai_gen_id }} {
    display: none;
  }

  .ai-category-arrow-{{ ai_gen_id }} {
    display: none;
  }

  @media screen and (min-width: 750px) {
    .ai-category-section-{{ ai_gen_id }} {
      padding: {{ block.settings.padding_top }}px 0 {{ block.settings.padding_bottom }}px;
    }

    .ai-category-title-front-{{ ai_gen_id }} {
      font-size: 48px;
      padding: 16px 40px;
    }

    .ai-category-grid-{{ ai_gen_id }} {
      display: none;
    }

    .ai-category-desktop-slider-{{ ai_gen_id }} {
      display: block;
      position: relative;
      max-width: 100%;
      margin: 0 auto;
      padding: 0 40px;
    }

    .ai-category-slider-wrapper-{{ ai_gen_id }} {
      overflow: hidden;
    }

    .ai-category-slider-track-{{ ai_gen_id }} {
      display: flex;
      gap: {{ block.settings.gap_desktop }}px;
      transition: transform 0.4s ease;
    }

    .ai-category-slider-item-{{ ai_gen_id }} {
      flex: 0 0 calc((100% - ({{ block.settings.gap_desktop }}px * 3)) / 4);
      min-width: 0;
    }

    .ai-category-arrow-{{ ai_gen_id }} {
      display: flex;
      position: absolute;
      top: 50%;
      transform: translateY(-50%);
      width: 40px;
      height: 40px;
      border-radius: 8px;
      background-color: {{ block.settings.arrow_background }};
      color: {{ block.settings.arrow_color }};
      border: 1px solid #E0E0E0;
      cursor: pointer;
      align-items: center;
      justify-content: center;
      z-index: 10;
      transition: all 0.3s ease;
      box-shadow: 0 4px 8px rgba(0, 0, 0, 0);
    }

    .ai-category-arrow-{{ ai_gen_id }}:hover {
      background-color: {{ block.settings.arrow_hover_background }};
      border-color: #CCCCCC;
      box-shadow: 0 4px 8px rgba(0, 0, 0, 0);
    }

    .ai-category-arrow-{{ ai_gen_id }}:disabled {
      opacity: 0.3;
      cursor: not-allowed;
    }

    .ai-category-arrow-prev-{{ ai_gen_id }} {
      left: 0px;
    }

    .ai-category-arrow-next-{{ ai_gen_id }} {
      right: 0px;
    }

    .ai-category-arrow-{{ ai_gen_id }} svg {
      width: 20px;
      height: 20px;
    }

    .ai-category-overlay-{{ ai_gen_id }} {
      font-size: 16px;
      padding: 16px;
    }
  }
{% endstyle %}

<div class="ai-category-section-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  {% if block.settings.section_title != blank %}
    <div class="ai-category-title-wrapper-{{ ai_gen_id }}">
      <div class="ai-category-title-inner-{{ ai_gen_id }}">
        <span class="ai-category-title-shadow-{{ ai_gen_id }}"></span>
        <span class="ai-category-title-edge-{{ ai_gen_id }}"></span>
        <span class="ai-category-title-front-{{ ai_gen_id }}">{{ block.settings.section_title }}</span>
      </div>
    </div>
  {% endif %}

  <div class="ai-category-grid-{{ ai_gen_id }}">
    {% for i in (1..12) %}
      {% assign collection_key = 'collection_' | append: i %}
      {% assign image_key = 'custom_image_' | append: i %}
      {% assign hide_key = 'hide_collection_' | append: i %}
      
      {% assign collection = block.settings[collection_key] %}
      {% assign custom_image = block.settings[image_key] %}
      {% assign hide_collection = block.settings[hide_key] %}

      {% unless hide_collection %}
        {% if collection != blank %}
          <div class="ai-category-item-{{ ai_gen_id }}">
            <a href="{{ collection.url }}" class="ai-category-link-{{ ai_gen_id }}">
              <div class="ai-category-image-wrapper-{{ ai_gen_id }}">
                {% if custom_image != blank %}
                  <img
                    src="{{ custom_image | image_url: width: 500 }}"
                    alt="{{ collection.title | escape }}"
                    class="ai-category-image-{{ ai_gen_id }}"
                    loading="lazy"
                    width="500"
                    height="500"
                  >
                {% elsif collection.featured_image != blank %}
                  <img
                    src="{{ collection.featured_image | image_url: width: 500 }}"
                    alt="{{ collection.title | escape }}"
                    class="ai-category-image-{{ ai_gen_id }}"
                    loading="lazy"
                    width="500"
                    height="500"
                  >
                {% else %}
                  <div class="ai-category-placeholder-{{ ai_gen_id }}">
                    {{ 'collection-1' | placeholder_svg_tag }}
                  </div>
                {% endif %}
              </div>
              {% if block.settings.show_overlay_text %}
                <div class="ai-category-overlay-{{ ai_gen_id }}">
                  {{ collection.title }}
                </div>
              {% endif %}
            </a>
          </div>
        {% endif %}
      {% endunless %}
    {% endfor %}
  </div>

  <category-slider-{{ ai_gen_id }} class="ai-category-desktop-slider-{{ ai_gen_id }}">
    <button class="ai-category-arrow-{{ ai_gen_id }} ai-category-arrow-prev-{{ ai_gen_id }}" aria-label="Previous">
      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
        <polyline points="15 18 9 12 15 6"></polyline>
      </svg>
    </button>

    <div class="ai-category-slider-wrapper-{{ ai_gen_id }}">
      <div class="ai-category-slider-track-{{ ai_gen_id }}">
        {% for i in (1..12) %}
          {% assign collection_key = 'collection_' | append: i %}
          {% assign image_key = 'custom_image_' | append: i %}
          {% assign hide_key = 'hide_collection_' | append: i %}
          
          {% assign collection = block.settings[collection_key] %}
          {% assign custom_image = block.settings[image_key] %}
          {% assign hide_collection = block.settings[hide_key] %}

          {% unless hide_collection %}
            {% if collection != blank %}
              <div class="ai-category-item-{{ ai_gen_id }} ai-category-slider-item-{{ ai_gen_id }}">
                <a href="{{ collection.url }}" class="ai-category-link-{{ ai_gen_id }}">
                  <div class="ai-category-image-wrapper-{{ ai_gen_id }}">
                    {% if custom_image != blank %}
                      <img
                        src="{{ custom_image | image_url: width: 500 }}"
                        alt="{{ collection.title | escape }}"
                        class="ai-category-image-{{ ai_gen_id }}"
                        loading="lazy"
                        width="500"
                        height="500"
                      >
                    {% elsif collection.featured_image != blank %}
                      <img
                        src="{{ collection.featured_image | image_url: width: 500 }}"
                        alt="{{ collection.title | escape }}"
                        class="ai-category-image-{{ ai_gen_id }}"
                        loading="lazy"
                        width="500"
                        height="500"
                      >
                    {% else %}
                      <div class="ai-category-placeholder-{{ ai_gen_id }}">
                        {{ 'collection-1' | placeholder_svg_tag }}
                      </div>
                    {% endif %}
                  </div>
                  {% if block.settings.show_overlay_text %}
                    <div class="ai-category-overlay-{{ ai_gen_id }}">
                      {{ collection.title }}
                    </div>
                  {% endif %}
                </a>
              </div>
            {% endif %}
          {% endunless %}
        {% endfor %}
      </div>
    </div>

    <button class="ai-category-arrow-{{ ai_gen_id }} ai-category-arrow-next-{{ ai_gen_id }}" aria-label="Next">
      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
        <polyline points="9 18 15 12 9 6"></polyline>
      </svg>
    </button>
  </category-slider-{{ ai_gen_id }}>
</div>

<script>
  (function() {
    class CategorySlider{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.currentIndex = 0;
        this.itemsPerView = 4;
        this.gap = {{ block.settings.gap_desktop }};
      }

      connectedCallback() {
        this.track = this.querySelector('.ai-category-slider-track-{{ ai_gen_id }}');
        this.items = this.querySelectorAll('.ai-category-slider-item-{{ ai_gen_id }}');
        this.prevButton = this.querySelector('.ai-category-arrow-prev-{{ ai_gen_id }}');
        this.nextButton = this.querySelector('.ai-category-arrow-next-{{ ai_gen_id }}');

        this.totalItems = this.items.length;
        this.maxIndex = Math.max(0, this.totalItems - this.itemsPerView);

        this.prevButton.addEventListener('click', () => this.slide(-1));
        this.nextButton.addEventListener('click', () => this.slide(1));

        this.updateButtons();
      }

      slide(direction) {
        this.currentIndex += direction;
        this.currentIndex = Math.max(0, Math.min(this.currentIndex, this.maxIndex));
        this.updateSlider();
      }

      updateSlider() {
        const itemWidth = this.items[0].offsetWidth;
        const offset = -(this.currentIndex * (itemWidth + this.gap));
        this.track.style.transform = 'translateX(' + offset + 'px)';
        this.updateButtons();
      }

      updateButtons() {
        this.prevButton.disabled = this.currentIndex === 0;
        this.nextButton.disabled = this.currentIndex >= this.maxIndex;
      }
    }

    customElements.define('category-slider-{{ ai_gen_id }}', CategorySlider{{ ai_gen_id }});
  })();
</script>

