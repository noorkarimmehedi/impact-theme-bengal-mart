      "label": "Overlay text color",
      "default": "#FFFFFF"
    },
    {
      "type": "range",
      "id": "overlay_background_opacity",
      "min": 0,
      "max": 100,
      "step": 5,
      "unit": "%",
      "label": "Overlay background opacity",
      "default": 60
    },
    {
      "type": "header",
      "content": "Border"
    },
    {
      "type": "range",
      "id": "border_width",
      "min": 0,
      "max": 5,
      "step": 1,
      "unit": "px",
      "label": "Border width",
      "default": 1
