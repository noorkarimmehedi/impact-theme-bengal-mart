{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-search-container-{{ ai_gen_id }} {
    display: block;
    width: 100%;
    padding: {{ block.settings.padding_top }}px 0 {{ block.settings.padding_bottom }}px;
    background: {{ block.settings.background_color }};
  }

  .ai-search-wrapper-{{ ai_gen_id }} {
    max-width: {{ block.settings.max_width }}px;
    margin: 0 auto;
    padding: 0;
  }

  .ai-search-heading-{{ ai_gen_id }} {
    text-align: {{ block.settings.heading_alignment }};
    color: {{ block.settings.heading_color }};
    font-size: {{ block.settings.heading_size }}px;
    font-weight: 700;
    letter-spacing: -0.02em;
    margin: 0 0 {{ block.settings.heading_spacing }}px 0;
    padding: 0 20px;
    background: linear-gradient(135deg, #7900FD 0%, #9D4EDD 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
  }

  .ai-search-form-{{ ai_gen_id }} {
    position: relative;
    width: 100%;
    padding: 0;
  }

  .ai-search-input-wrapper-{{ ai_gen_id }} {
    position: relative;
    display: flex;
    align-items: center;
    background: {{ block.settings.input_background }};
    border: 2px solid transparent;
    border-radius: {{ block.settings.input_border_radius }}px;
    overflow: hidden;
    margin: 0 20px;
    box-shadow: 0 4px 20px rgba(121, 0, 253, 0.08);
    transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
  }

  .ai-search-input-wrapper-{{ ai_gen_id }}:focus-within {
    border-color: #7900FD;
    box-shadow: 0 8px 30px rgba(121, 0, 253, 0.2);
    transform: translateY(-2px);
  }

  .ai-search-icon-{{ ai_gen_id }} {
    padding: 0 18px;
    display: flex;
    align-items: center;
    color: #7900FD;
    transition: transform 0.3s ease;
  }

  .ai-search-input-wrapper-{{ ai_gen_id }}:focus-within .ai-search-icon-{{ ai_gen_id }} {
    transform: scale(1.1);
  }

  .ai-search-icon-{{ ai_gen_id }} svg {
    width: {{ block.settings.icon_size }}px;
    height: {{ block.settings.icon_size }}px;
    stroke-width: 2.5;
  }

  .ai-search-input-{{ ai_gen_id }} {
    flex: 1;
    padding: {{ block.settings.input_padding }}px 18px;
    border: none;
    background: transparent;
    font-size: {{ block.settings.input_font_size }}px;
    font-weight: 500;
    color: {{ block.settings.input_text_color }};
    outline: none;
  }

  .ai-search-input-{{ ai_gen_id }}::placeholder {
    color: {{ block.settings.placeholder_color }};
    font-weight: 400;
  }

  .ai-search-clear-{{ ai_gen_id }} {
    padding: 0 18px;
    background: none;
    border: none;
    cursor: pointer;
    display: none;
    align-items: center;
    color: #9CA3AF;
    transition: all 0.2s ease;
  }

  .ai-search-clear-{{ ai_gen_id }}.active {
    display: flex;
  }

  .ai-search-clear-{{ ai_gen_id }}:hover {
    color: #7900FD;
    transform: rotate(90deg);
  }

  .ai-search-clear-{{ ai_gen_id }} svg {
    width: 20px;
    height: 20px;
  }

  .ai-search-submit-{{ ai_gen_id }} {
    padding: 0 20px 0 12px;
    background: linear-gradient(135deg, #7900FD 0%, #9D4EDD 100%);
    border: none;
    cursor: pointer;
    display: flex;
    align-items: center;
    color: #ffffff;
    font-weight: 600;
    font-size: 14px;
    gap: 6px;
    transition: all 0.3s ease;
    height: 100%;
    letter-spacing: -0.01em;
  }

  .ai-search-submit-{{ ai_gen_id }}:hover {
    background: linear-gradient(135deg, #6800E0 0%, #8C3DCB 100%);
    transform: translateX(-2px);
  }

  .ai-search-submit-{{ ai_gen_id }} svg {
    width: 18px;
    height: 18px;
  }

  .ai-search-suggestions-{{ ai_gen_id }} {
    position: absolute;
    top: calc(100% + 12px);
    left: 20px;
    right: 20px;
    background: {{ block.settings.suggestions_background }};
    border: 1px solid #F3F4F6;
    border-radius: {{ block.settings.suggestions_border_radius }}px;
    box-shadow: 0 20px 60px rgba(0, 0, 0, 0.12);
    max-height: 480px;
    overflow-y: auto;
    z-index: 100;
    display: none;
    backdrop-filter: blur(10px);
  }

  .ai-search-suggestions-{{ ai_gen_id }}.active {
    display: block;
    animation: slideDown{{ ai_gen_id }} 0.3s cubic-bezier(0.4, 0, 0.2, 1);
  }

  @keyframes slideDown{{ ai_gen_id }} {
    from {
      opacity: 0;
      transform: translateY(-10px);
    }
    to {
      opacity: 1;
      transform: translateY(0);
    }
  }

  .ai-search-loading-{{ ai_gen_id }} {
    padding: 32px;
    text-align: center;
    color: #6B7280;
    font-weight: 500;
  }

  .ai-search-no-results-{{ ai_gen_id }} {
    padding: 40px 20px;
    text-align: center;
    color: #6B7280;
    font-weight: 500;
  }

  .ai-search-suggestion-item-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    gap: 16px;
    padding: 16px 20px;
    text-decoration: none;
    color: {{ block.settings.text_color }};
    border-bottom: 1px solid #F3F4F6;
    transition: all 0.2s cubic-bezier(0.4, 0, 0.2, 1);
    position: relative;
  }

  .ai-search-suggestion-item-{{ ai_gen_id }}::before {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
    width: 3px;
    background: #7900FD;
    transform: scaleY(0);
    transition: transform 0.2s ease;
  }

  .ai-search-suggestion-item-{{ ai_gen_id }}:last-child {
    border-bottom: none;
  }

  .ai-search-suggestion-item-{{ ai_gen_id }}:hover {
    background: linear-gradient(90deg, rgba(121, 0, 253, 0.04) 0%, transparent 100%);
    transform: translateX(4px);
  }

  .ai-search-suggestion-item-{{ ai_gen_id }}:hover::before {
    transform: scaleY(1);
  }

  .ai-search-suggestion-image-{{ ai_gen_id }} {
    width: 70px;
    height: 70px;
    flex-shrink: 0;
    border-radius: 12px;
    overflow: hidden;
    background: linear-gradient(135deg, #F3F4F6 0%, #E5E7EB 100%);
    display: flex;
    align-items: center;
    justify-content: center;
    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.06);
    transition: transform 0.2s ease;
  }

  .ai-search-suggestion-item-{{ ai_gen_id }}:hover .ai-search-suggestion-image-{{ ai_gen_id }} {
    transform: scale(1.05);
  }

  .ai-search-suggestion-image-{{ ai_gen_id }} img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    display: block;
  }

  .ai-search-suggestion-info-{{ ai_gen_id }} {
    flex: 1;
    min-width: 0;
  }

  .ai-search-suggestion-title-{{ ai_gen_id }} {
    font-size: 15px;
    font-weight: 600;
    margin: 0 0 6px 0;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #111827;
    letter-spacing: -0.01em;
  }

  .ai-search-suggestion-price-{{ ai_gen_id }} {
    font-size: 15px;
    font-weight: 700;
    color: #7900FD;
    letter-spacing: -0.01em;
  }

  .ai-search-view-all-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 8px;
    padding: 20px;
    text-align: center;
    background: linear-gradient(135deg, #7900FD 0%, #9D4EDD 100%);
    color: #ffffff;
    text-decoration: none;
    font-weight: 600;
    font-size: 15px;
    letter-spacing: -0.01em;
    border-top: 1px solid rgba(121, 0, 253, 0.1);
    transition: all 0.3s ease;
  }

  .ai-search-view-all-{{ ai_gen_id }}:hover {
    background: linear-gradient(135deg, #6800E0 0%, #8C3DCB 100%);
    transform: translateY(-1px);
  }

  .ai-search-view-all-{{ ai_gen_id }} svg {
    width: 18px;
    height: 18px;
    transition: transform 0.3s ease;
  }

  .ai-search-view-all-{{ ai_gen_id }}:hover svg {
    transform: translateX(4px);
  }

  .ai-search-suggestions-{{ ai_gen_id }}::-webkit-scrollbar {
    width: 6px;
  }

  .ai-search-suggestions-{{ ai_gen_id }}::-webkit-scrollbar-track {
    background: #F9FAFB;
  }

  .ai-search-suggestions-{{ ai_gen_id }}::-webkit-scrollbar-thumb {
    background: #D1D5DB;
    border-radius: 3px;
  }

  .ai-search-suggestions-{{ ai_gen_id }}::-webkit-scrollbar-thumb:hover {
    background: #7900FD;
  }

  @media screen and (max-width: 749px) {
    .ai-search-heading-{{ ai_gen_id }} {
      font-size: calc({{ block.settings.heading_size }}px * 0.8);
      padding: 0 8px;
    }

    .ai-search-input-wrapper-{{ ai_gen_id }} {
      margin: 0 8px;
    }

    .ai-search-suggestions-{{ ai_gen_id }} {
      left: 8px;
      right: 8px;
    }

    .ai-search-input-{{ ai_gen_id }} {
      font-size: 16px !important;
    }

    .ai-search-suggestion-item-{{ ai_gen_id }} {
      padding: 14px 16px;
      gap: 12px;
    }

    .ai-search-suggestion-image-{{ ai_gen_id }} {
      width: 60px;
      height: 60px;
    }
  }
{% endstyle %}

<product-search-{{ ai_gen_id }}
  class="ai-search-container-{{ ai_gen_id }}"
  {{ block.shopify_attributes }}
  data-min-chars="{{ block.settings.min_characters }}"
  data-max-results="{{ block.settings.max_results }}"
>
  <div class="ai-search-wrapper-{{ ai_gen_id }}">
    {% if block.settings.heading != blank %}
      <h2 class="ai-search-heading-{{ ai_gen_id }}">{{ block.settings.heading }}</h2>
    {% endif %}

    <div class="ai-search-form-{{ ai_gen_id }}">
      <div class="ai-search-input-wrapper-{{ ai_gen_id }}">
        <span class="ai-search-icon-{{ ai_gen_id }}">
          <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
            <circle cx="11" cy="11" r="8"></circle>
            <path d="m21 21-4.35-4.35"></path>
          </svg>
        </span>
        <input
          type="text"
          class="ai-search-input-{{ ai_gen_id }}"
          placeholder="{{ block.settings.placeholder }}"
          autocomplete="off"
          aria-label="Search products"
          aria-autocomplete="list"
          aria-controls="ai-search-suggestions-{{ ai_gen_id }}"
        >
        <button
          type="button"
          class="ai-search-clear-{{ ai_gen_id }}"
          aria-label="Clear search"
        >
          <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
            <line x1="18" y1="6" x2="6" y2="18"></line>
            <line x1="6" y1="6" x2="18" y2="18"></line>
          </svg>
        </button>
        <button
          type="button"
          class="ai-search-submit-{{ ai_gen_id }}"
          aria-label="Search"
        >
          Search
          <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
            <line x1="5" y1="12" x2="19" y2="12"></line>
            <polyline points="12 5 19 12 12 19"></polyline>
          </svg>
        </button>
      </div>

      <div
        id="ai-search-suggestions-{{ ai_gen_id }}"
        class="ai-search-suggestions-{{ ai_gen_id }}"
        role="listbox"
      ></div>
    </div>
  </div>
</product-search-{{ ai_gen_id }}>

<script>
  (function() {
    class ProductSearch{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.input = this.querySelector('.ai-search-input-{{ ai_gen_id }}');
        this.clearBtn = this.querySelector('.ai-search-clear-{{ ai_gen_id }}');
        this.submitBtn = this.querySelector('.ai-search-submit-{{ ai_gen_id }}');
        this.suggestions = this.querySelector('.ai-search-suggestions-{{ ai_gen_id }}');
        this.minChars = parseInt(this.dataset.minChars) || 2;
        this.maxResults = parseInt(this.dataset.maxResults) || 5;
        this.searchTimeout = null;
        this.currentRequest = null;
      }

      connectedCallback() {
        this.input.addEventListener('input', this.handleInput.bind(this));
        this.input.addEventListener('keypress', this.handleKeyPress.bind(this));
        this.clearBtn.addEventListener('click', this.clearSearch.bind(this));
        this.submitBtn.addEventListener('click', this.handleSubmit.bind(this));
        document.addEventListener('click', this.handleClickOutside.bind(this));
      }

      disconnectedCallback() {
        document.removeEventListener('click', this.handleClickOutside.bind(this));
      }

      handleInput(event) {
        const query = event.target.value.trim();

        if (query.length > 0) {
          this.clearBtn.classList.add('active');
        } else {
          this.clearBtn.classList.remove('active');
        }

        clearTimeout(this.searchTimeout);

        if (query.length < this.minChars) {
          this.hideSuggestions();
          return;
        }

        this.searchTimeout = setTimeout(() => {
          this.searchProducts(query);
        }, 300);
      }

      async searchProducts(query) {
        if (this.currentRequest) {
          this.currentRequest.abort();
        }

        this.showLoading();
        this.currentRequest = new AbortController();

        try {
          const response = await fetch(
            `/search/suggest.json?q=${encodeURIComponent(query)}&resources[type]=product&resources[limit]=${this.maxResults}`,
            { signal: this.currentRequest.signal }
          );

          if (!response.ok) throw new Error('Search failed');

          const data = await response.json();
          this.displayResults(data.resources.results.products || [], query);
        } catch (error) {
          if (error.name !== 'AbortError') {
            this.showError();
          }
        } finally {
          this.currentRequest = null;
        }
      }

      showLoading() {
        this.suggestions.innerHTML = `
          <div class="ai-search-loading-{{ ai_gen_id }}">
            <svg width="24" height="24" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg" fill="#7900FD">
              <circle cx="4" cy="12" r="3">
                <animate attributeName="opacity" dur="1s" values="0;1;0" repeatCount="indefinite" begin="0.1"/>
              </circle>
              <circle cx="12" cy="12" r="3">
                <animate attributeName="opacity" dur="1s" values="0;1;0" repeatCount="indefinite" begin="0.2"/>
              </circle>
              <circle cx="20" cy="12" r="3">
                <animate attributeName="opacity" dur="1s" values="0;1;0" repeatCount="indefinite" begin="0.3"/>
              </circle>
            </svg>
          </div>
        `;
        this.suggestions.classList.add('active');
      }

      showError() {
        this.suggestions.innerHTML = `
          <div class="ai-search-no-results-{{ ai_gen_id }}">
            Something went wrong. Please try again.
          </div>
        `;
      }

      displayResults(products, query) {
        if (products.length === 0) {
          this.suggestions.innerHTML = `
            <div class="ai-search-no-results-{{ ai_gen_id }}">
              No products found for "${this.escapeHtml(query)}"
            </div>
          `;
          this.suggestions.classList.add('active');
          return;
        }

        let html = '';

        products.forEach(product => {
          const imageUrl = product.image ? this.getImageUrl(product.image) : '';
          const price = this.formatPrice(product.price);

          html += `
            <a href="${product.url}" class="ai-search-suggestion-item-{{ ai_gen_id }}" role="option">
              <div class="ai-search-suggestion-image-{{ ai_gen_id }}">
                ${imageUrl ? `<img src="${imageUrl}" alt="${this.escapeHtml(product.title)}" loading="lazy">` : ''}
              </div>
              <div class="ai-search-suggestion-info-{{ ai_gen_id }}">
                <h3 class="ai-search-suggestion-title-{{ ai_gen_id }}">${this.escapeHtml(product.title)}</h3>
                <div class="ai-search-suggestion-price-{{ ai_gen_id }}">${price}</div>
              </div>
            </a>
          `;
        });

        html += `
          <a href="/search?q=${encodeURIComponent(query)}" class="ai-search-view-all-{{ ai_gen_id }}">
            View all results
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
              <line x1="5" y1="12" x2="19" y2="12"></line>
              <polyline points="12 5 19 12 12 19"></polyline>
            </svg>
          </a>
        `;

        this.suggestions.innerHTML = html;
        this.suggestions.classList.add('active');
      }

      getImageUrl(imageUrl) {
        if (!imageUrl) return '';
        if (imageUrl.includes('?')) {
          return imageUrl.split('?')[0] + '?width=140';
        }
        return imageUrl + '?width=140';
      }

      formatPrice(priceInCents) {
        // If it's already a string, return it
        if (typeof priceInCents === 'string') {
          return priceInCents;
        }

        // Convert cents to dollars
        const priceInDollars = priceInCents / 100;

        // Use Shopify.formatMoney if available
        if (typeof Shopify !== 'undefined' && Shopify.formatMoney) {
          return Shopify.formatMoney(priceInCents, '{{ shop.money_format }}');
        }

        // Fallback formatting
        return '{{ shop.currency }}' + ' ' + priceInDollars.toFixed(2);
      }

      escapeHtml(text) {
        const div = document.createElement('div');
        div.textContent = text;
        return div.innerHTML;
      }

      clearSearch() {
        this.input.value = '';
        this.clearBtn.classList.remove('active');
        this.hideSuggestions();
        this.input.focus();
      }

      hideSuggestions() {
        this.suggestions.classList.remove('active');
        this.suggestions.innerHTML = '';
      }

      handleClickOutside(event) {
        if (!this.contains(event.target)) {
          this.hideSuggestions();
        }
      }

      handleKeyPress(event) {
        if (event.key === 'Enter') {
          event.preventDefault();
          this.handleSubmit();
        }
      }

      handleSubmit() {
        const query = this.input.value.trim();
        if (query.length >= this.minChars) {
          window.location.href = `/search?q=${encodeURIComponent(query)}`;
        }
      }
    }

    customElements.define('product-search-{{ ai_gen_id }}', ProductSearch{{ ai_gen_id }});
  })();
</script>

